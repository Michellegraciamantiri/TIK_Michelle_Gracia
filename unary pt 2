#include <iostream>
#include <math.h>
using namespace std;

int main(){
float a = sqrt(16);//menghitung akar kuadrat 
float b = cbrt(27);//akar kubik
float c = pow (4,2);//menghitung pangkat
float d = sin (10);//menghitung sin
float e = cos (10);//menghitung cos
float f = tan (10);//menghitung tan
float g = round (3.5);// membulatkan
   cout<<a<<endl;
   cout<<b<<endl;
   cout<<c<<endl;
   cout<<d<<endl;
   cout<<e<<endl;
   cout<<f<<endl;
   cout<<g<<endl;
   return 0;
}
